{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["eslint:recommended", "plugin:react/recommended", "prettier"],
    "settings": {
        "react": {
            "version": "detect"
        }
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": ["react", "prettier"],
    "globals": {
        "process": true
    },
    "rules": {
        "prettier/prettier": "error",
        "import/no-anonymous-default-export": 0,
        "react/display-name": 0,
        "react/prop-types": 0
    }
}  
     